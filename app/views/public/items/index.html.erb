<h1>商品一覧（全 <%= @quantity %> 件）</h1>
 <% @items.each do |item| %>
  <%= link_to public_item_path(item.id) do %>
    <%= image_tag item.get_product_image(200,200)%>
  <% end %><br>
    <%= item.itemname %><br>
    <%= item.price %>円
    <td class="item-evaluation" data-score="<%= item.evaluation %>"></td>

  <% end %>
  <script>
  $('.item-evaluation').raty({
    readOnly: true,
    score: function() {
      return $(this).attr('data-score');
    },
    path: '/assets/'
  });
</script>
